  "bugs": {
    "url": "https://github.com/urbanairship/frock/issues"
  },
  "homepage": "https://github.com/urbanairship/frock#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "os-tmpdir": "^1.0.1",
    "portfinder": "^0.4.0",
    "proxyquire": "^1.7.3",
    "rimraf": "^2.4.3",
    "standard": "^5.1.0",
    "tape": "^4.0.0",
    "through": "^2.3.8"
